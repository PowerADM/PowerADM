{% extends '@EasyAdmin/page/content.html.twig' %}

{% block page_title %}PowerDNS configuration{% endblock %}
{% block main %}
	<table class="table datagrid">
		<thead>
			<tr>
				<th><span>Key</span></th>
				<th><span>Value</span></th>
			</tr>
		</thead>
		<tbody>
			{% for item in configuration %}
				<tr>
					<td>{{ item.name }}</td>
					<td>{{ item.value }}</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>
{% endblock main %}