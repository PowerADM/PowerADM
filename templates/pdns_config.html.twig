{% extends '@EasyAdmin/page/content.html.twig' %}

{% block page_title %}
	{% trans %}pdns.configuration.pdns_configuration{% endtrans %}
{% endblock %}
{% block main %}
	<table class="table datagrid">
		<thead>
			<tr>
				<th><span>
					{% trans %}pdns.configuration.key{% endtrans %}
				</span></th>
				<th><span>
					{% trans %}pdns.configuration.value{% endtrans %}
				</span></th>
			</tr>
		</thead>
		<tbody>
			{% for item in configuration %}
				<tr>
					<td>{{ item.name }}</td>
					<td>{{ item.value }}</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>
{% endblock main %}